{
  "parser": "@babel/eslint-parser",
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "requireConfigFile": false
  },
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": ["error", {
      "js": "ignorePackages"
      }
    ],
    "no-underscore-dangle": [0],
    "no-restricted-syntax": [0]
  }
}
